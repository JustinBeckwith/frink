<?xml version="1.0" encoding="utf-8"?>
<s:SkinnableContainer xmlns:fx="http://ns.adobe.com/mxml/2009" 
		  xmlns:s="library://ns.adobe.com/flex/spark"
		  xmlns:mx="library://ns.adobe.com/flex/mx" backgroundAlpha="0.0">
	<fx:Script>
		<![CDATA[
			
			[Bindable]
			public var icon_normal : Object;
			
			[Bindable]
			public var icon_selected : Object;
			
		]]>
	</fx:Script>
	
	<s:states>
		<s:State name="normal" />
		<s:State name="selected" />
	</s:states>
	
	<s:Image id="iconImage" source.normal="{icon_normal}" source.selected="{icon_selected}" />
	
</s:SkinnableContainer>
